  "license": "MIT",
  "bugs": {
    "url": "https://github.com/austin0209/npacker/issues"
  },
  "homepage": "https://github.com/austin0209/npacker#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "esm": "^3.2.25",
    "jimp": "^0.14.0",
    "minimist": "^1.2.6"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.0",
    "@types/node": "^14.14.44",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  }
