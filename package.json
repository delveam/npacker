{
  "name": "npacker",
  "version": "1.0.0",
  "description": "A sprite packer.",
  "main": "./build/index.js",
  "bin": {
    "npack": "./build/index.js"
  },
  "scripts": {
    "start": "node ./build/index.js",
    "dev": "tsc -p ./tsconfig.dev.json && node ./dev/index.js",
    "build": "tsc -p ./tsconfig.base.json",
    "format": "prettier --write --use-tabs src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/austin0209/npacker.git"
  },
